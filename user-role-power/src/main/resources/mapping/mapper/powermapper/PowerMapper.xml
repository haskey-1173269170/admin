<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jk.power.mapper.PowerMapper">
    <select id="queryPower" resultType="com.jk.power.model.Power" parameterType="int">
        select * from t_power where pid = #{pid}
    </select>
    <select id="queryAllPower" resultType="com.jk.power.model.Power">

        select *from t_power
    </select>
</mapper>